<div class="container m-3">
    <div class="row">
        <div class="col-3">
        <%= render partial: "sidebar" %>
        </div>
        <div class= "col-9">
            <table class="table table-striped">
                <thead>
                <tr>
                    <th>氏名</th>
                    <th>メールアドレス</th>
                </tr>
                </thead>
                <tbody>
                <% @users.each do |user| %>
                    <tr>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                    </tr>
                <% end %>
                </tbody>
            </table>
        </div>
    </div>
</div>
